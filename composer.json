{
    "name": "wrbss/lib-composer-boilerplate",
    "description": "A boilerplate composer library",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-0": {
            "Wrbss\\HelloWorld\\": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Wrbss\\HelloWorld\\": "tests"
        }
    }
}
